<%- include('partials/h-user.ejs') %>
    <div class="bg-dark" style="padding: 10 0;">
        <div id="carouselExampleControls" class="carousel slide" data-mdb-ride="carousel">
            <div class="carousel-inner">
                <% slide.forEach(function(slide){ %>
                    <div class="<%= slide.class%>">
                        <img src="<%= slide.image %>" class="d-block w-20 image-slide" />
                    </div>
                    <% }); %>
            </div>
            <button class="carousel-control-prev" type="button" data-mdb-target="#carouselExampleControls"
                data-mdb-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-mdb-target="#carouselExampleControls"
                data-mdb-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>

    <div style="padding: 20 0; background: #729086;">
        <h4 class="text-center text-white">TOP MUSIC INTERNATIONAL</h4>
        <p class="text-center text-white">Download the hit songs from the Ciambella music charts before anyone else
            here.</p>
    </div>

    <div style="background-image: url('./images/background.jpg');">
        <div class="container">
            <div style="padding: 20 0;">
                <!-- <h5 class="text-center" style="color: #97918b;">Top 20</h5> -->
                <!-- <h6 class="text-center" style="color: #97918b;">Update March 15, 2021</h6> -->
            </div>

            <div class="row">
                <div class="col-md-2">
                    <div>
                        <!-- <a href="/artist/melody/<%= music.music %>"> -->
                        <img src="./uploads/image-1623164088868.jpg" class="img-fluid" />
                        </a>
                    </div>
                </div>



                <div class="col-md-10">
                    <div class="row">
                        <% music.forEach(function(music){ %>
                            <div class="col-md-5">
                                <div class="row">
                                    <div class="col-md-2 align-self-center">
                                        <div>
                                            <h1 class="text-center text-white"></h1>
                                        </div>
                                    </div>
                                    <div class="col-md-5">
                                        <div class="text-center">
                                            <a href="/artist/melody/<%= music.music %>">
                                                <img src="<%= music.image %>" class="img-fluid w-100 image-item" />
                                            </a>
                                        </div>

                                    </div>
                                    <div class="col-md-5">
                                        <div class="row">
                                            <div class="col-md" style="color: #73565e">
                                                <a href="/artist/melody/<%= music.music %>">
                                                <strong>
                                                    <%= music.music %>
                                                </strong></a>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md text-white">
                                                <b>
                                                    <%= music.name %>
                                            </b>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% }); %>
                    </div>





                </div>
            </div>

        </div>
    </div>

    </div>
    <style>
        .col-md-5,
        .col-md-6 {
            font-family: "Avant Garde", Avantgarde, "Century Gothic", CenturyGothic, "AppleGothic", sans-serif;
            -webkit-box-flex: 0;
            -ms-flex: 0 0 auto;
